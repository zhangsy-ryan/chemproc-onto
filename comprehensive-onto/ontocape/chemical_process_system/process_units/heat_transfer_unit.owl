  <owl:Class rdf:ID="Axiom_HeatLeak">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasHeatLeak"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/chemical_process_system/CPS_behavior/behavior.owl#HeatTransferRate">
                <rdfs:subClassOf>
                  <owl:Restriction>
                    <owl:hasValue rdf:resource="http://www.theworldavatar.com/ontology/ontocape/chemical_process_system/CPS_behavior/behavior.owl#interface_heat_conduction"/>
                    <owl:onProperty rdf:resource="http://www.theworldavatar.com/ontology/ontocape/upper_level/technical_system.owl#isInfluencedBy"/>
                  </owl:Restriction>
                </rdfs:subClassOf>
              </rdf:Description>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Class rdf:ID="HeatEnvironmentConnection"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith>
      <owl:Class rdf:ID="Axiom_HeatLoss"/>
    </owl:disjointWith>
  </owl:Class>
