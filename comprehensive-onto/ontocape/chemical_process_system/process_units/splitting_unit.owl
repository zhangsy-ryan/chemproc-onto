            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:allValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <rdf:Description rdf:about="http://www.theworldavatar.com/ontology/ontocape/chemical_process_system/CPS_function/process.owl#ProcessState"/>
