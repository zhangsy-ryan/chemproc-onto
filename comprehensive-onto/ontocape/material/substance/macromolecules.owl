  <polymers:End-Group rdf:ID="End-Group_of_Polyethylene__glycol">
    <substance:molecularFormula rdf:datatype="http://www.w3.org/2001/XMLSchema#string">·CH2-OH</substance:molecularFormula>
  </polymers:End-Group>
  <polymers:Macromolecule rdf:ID="Polyethylene__glycol">
    <substance:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PEO</substance:name>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">44.05          C2H4O</rdfs:comment>
    <substance:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PEG</substance:name>
