    <substance:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Polyethylene glycol</substance:name>
    <substance:molecularFormula rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HO-(CH2-CH2-O)n-H</substance:molecularFormula>
    <substance:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Polyethylene oxide</substance:name>
