  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/2006/time#TemporalEntity"/>
  <owl:Class rdf:about="https://saref.etsi.org/core/ToggleCommand">
    <skos:historyNote xml:lang="en">V3.2.1: saref:ToggleCommand is deprecated: may be defined as an instance of saref:Command in the next major revision of SAREF</skos:historyNote>
    <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
