    >true</owl:deprecated>
    <rdfs:label xml:lang="en">Actuating function</rdfs:label>
    <rdfs:comment xml:lang="en">A function that allows to transmit data to actuators, such as level settings (e.g., temperature) or binary switching (e.g., open/close, on/off)</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/Function"/>
    </rdfs:subClassOf>
  </owl:Class>
